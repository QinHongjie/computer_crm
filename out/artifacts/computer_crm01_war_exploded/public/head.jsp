<%@ page pageEncoding="UTF-8" %>
<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<!-- 上述3个meta标签*必须*放在最前面，任何其他内容都*必须*跟随其后！ -->
<%-- <title><%=request.getParameter("title") %></title> --%>
<link rel="icon" href="<%=request.getContextPath() %>/static/img/page_icon.png" />
<!-- Bootstrap -->
<link href="<%=request.getContextPath() %>/static/bootstrap3.0/css/bootstrap.min.css" rel="stylesheet">
<!-- jQuery (Bootstrap 的所有 JavaScript 插件都依赖 jQuery，所以必须放在前边) -->
<script src="<%=request.getContextPath() %>/static/bootstrap3.0/js/jquery.min.js"></script>
<!-- 加载 Bootstrap 的所有 JavaScript 插件。你也可以根据需要只加载单个插件。 -->
<script src="<%=request.getContextPath() %>/static/bootstrap3.0/js/bootstrap.min.js"></script>
<!-- bootstrap dialog -->
<link href="<%=request.getContextPath() %>/static/bootstrap3-dialog/css/bootstrap-dialog.min.css" rel="stylesheet" />
<script src="<%=request.getContextPath() %>/static/bootstrap3-dialog/js/bootstrap-dialog.min.js"></script>
<!--[if lt IE 9]>
<script src="https://cdn.jsdelivr.net/npm/html5shiv@3.7.3/dist/html5shiv.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/respond.js@1.4.2/dest/respond.min.js"></script>
<![endif]-->

<!-- 自定义的css文件 -->
<link href="<%=request.getContextPath() %>/static/css/index.css" rel="stylesheet">
<link href="<%=request.getContextPath() %>/static/css/animation.css" rel="stylesheet">

<!-- 自定义的js文件 -->
<script src="<%=request.getContextPath() %>/static/js/animation.js"></script>




